<h2>Available drones</h2>
{{#each drones}}
  <div class='showcase'>
    <a href='/drones/{{this._id}}/edit'>
      <h3>{{this.name}}</h3>
    </a>
    <ul>
      <li>
        Propellers:
        {{this.propellers}}
      </li>
      <li>
        Max Speed:
        {{this.maxSpeed}}
      </li>
    </ul>
    <form action='/drones/{{this._id}}/delete' method='POST'>
      <button>Delete</button>
    </form>
  </div>
{{/each}}